{
	"name": "coora-docs",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"docusaurus": "docusaurus --config ./src/docusaurus.config.js",
		"start": "docusaurus start --config ./src/docusaurus.config.js",
		"https": "docusaurus start --config ./src/docusaurus.config.js --host 0.0.0.0",
		"build": "docusaurus build --config ./src/docusaurus.config.js",
		"swizzle": "docusaurus swizzle --config ./src/docusaurus.config.js",
		"deploy": "docusaurus deploy --config ./src/docusaurus.config.js",
		"clear": "docusaurus clear --config ./src/docusaurus.config.js",
		"serve": "docusaurus serve --config ./src/docusaurus.config.js",
		"write-translations": "docusaurus write-translations --config ./src/docusaurus.config.js",
		"write-heading-ids": "docusaurus write-heading-ids --config ./src/docusaurus.config.js",
		"typecheck": "tsc"
	},
	"dependencies": {
		"@docusaurus/core": "2.2.0",
		"@docusaurus/preset-classic": "2.2.0",
		"@mdx-js/react": "^1.6.22",
		"clsx": "^1.2.1",
		"prism-react-renderer": "^1.3.5",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@docusaurus/module-type-aliases": "2.2.0",
		"@tsconfig/docusaurus": "^1.0.5",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"eslint": "^8.27.0",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-unicorn": "^44.0.2",
		"typescript": "^4.7.4"
	},
	"browserslist": {
