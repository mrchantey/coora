hello:
	curl http://192.168.86.222/data \
	-X POST \
	-d hello
wasm url crate:
	curl http://192.168.86.222/{{url}} \
	-X POST \
	--data-binary @../{{crate}}/target/wasm32-unknown-unknown/release/{{crate}}.wasm

sketch:
	just -f jf_post wasm sketch wasm_sketch

# stress:
# while true; do \
# just -f jf_post sketch; \
# done \
stress:
	while true; do \
		just -f jf_post sketch; \
		echo; \
		sleep 1; \
	done \